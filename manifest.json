{
    "name": "Auto Scroll",
    "description": "Scroll the length of a page for a specified duration",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": ["activeTab", "tabs", "scripting"],
    "background": {
        "service_worker": "background.js"
    }
}
